@model IEnumerable<Phd.Models.DictionaryStatusAvailability>

@{
    ViewData["Title"] = "Создать";
}

<h2>Справочник статусов</h2>

<p>
    <a asp-action="Create">Создать</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ValueRus)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ValueKaz)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ValueEng)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ValueRus)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ValueKaz)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ValueEng)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Править</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Детали</a> |
                <!--<a asp-action="Delete" asp-route-id="@item.Id">Delete</a>-->
            </td>
        </tr>
}
    </tbody>
</table>
