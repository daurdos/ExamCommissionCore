@model Phd.ViewModels.StudentGradeViewModel

@{
    ViewData["Title"] = "Получить результаты";
}

<h2>Общая оценка</h2>

@{ var aver = Model.AverageGrade;}





<div><strong><b><em>@Html.DisplayFor(model => model.AverageGrade) %</em></b></strong></div>

<div><strong><b><em>@Html.DisplayFor(model => model.StudentName) </em></b></strong></div>
<div><strong><b><em>@Html.DisplayFor(model => model.MajorCypher) </em></b></strong></div>



@foreach (var item in Model.Users)
{
